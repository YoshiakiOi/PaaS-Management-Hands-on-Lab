@{
    ViewData["Title"] = "File Upload";
}

<h2>File Upload</h2>

<form action="SaveImages" method="POST" enctype="multipart/form-data">

<div class="form-group">
    <label for="inputFile">File Input</label>
    <input class="form-control-file" type="file" name="inputFiles" id="inputFiles" multiple>
</div>

<hr />

<button type="submit" class="btn btn-primary">Submit</button>

</form>